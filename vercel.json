{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://work-papa.onrender.com/:path*"
    }
  ],
  "regions": ["iad1"] 
}
